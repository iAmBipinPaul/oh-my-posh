{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 4,
  "final_space": true,
  "console_title_template": "{{ .Folder }} | {{ .UserName }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#F8FAFC",
          "background": "#0F172A",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if .WSL }}\ue712{{ else }}\ue70f{{ end }} "
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#F8FAFC",
          "background": "#3D1A99",
          "background_templates": [
            "{{ if .Root }}#EF4444{{ end }}"
          ],
          "template": " {{ if .Root }}\uf0e7 ADMIN {{ else }}\uf2bd {{ end }}{{ .UserName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#F8FAFC",
          "background": "#764AF1",
          "template": " \uf07c {{ .Path }} ",
          "options": {
            "style": "agnoster_short",
            "max_depth": 3
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#0F172A",
          "background": "#10B981",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#F59E0B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#EC4899{{ end }}",
            "{{ if gt .Ahead 0 }}#06B6D4{{ end }}",
            "{{ if gt .Behind 0 }}#A78BFA{{ end }}"
          ],
          "template": " \ue725 {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "options": {
            "fetch_status": true,
            "fetch_stash_count": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#F8FAFC",
          "background": "#0EA5E9",
          "leading_diamond": "\ue0b2",
          "template": " \uf017 {{ .CurrentDate | date \"3:04 PM\" }} "
        },
        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#F8FAFC",
          "background": "#475569",
          "template": " \uf489 {{ .Name }} "
        },
        {
          "type": "sysinfo",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#F8FAFC",
          "background": "#64748B",
          "template": " \uf85a {{ round .PhysicalPercentUsed .Precision }}% "
        },
        {
          "type": "battery",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#F8FAFC",
          "background": "#10B981",
          "background_templates": [
            "{{ if lt .Percentage 20 }}#EF4444{{ end }}",
            "{{ if and (ge .Percentage 20) (lt .Percentage 50) }}#F59E0B{{ end }}"
          ],
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }} "
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#F8FAFC",
          "background": "#10B981",
          "template": " \ue718 {{ .Full }} "
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#F8FAFC",
          "background": "#3776AB",
          "template": " \ue235 {{ .Full }} "
        },
        {
          "type": "go",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#F8FAFC",
          "background": "#00ADD8",
          "template": " \ue627 {{ .Full }} "
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#F8FAFC",
          "background": "#8B5CF6",
          "template": " \ue77f {{ .Full }} "
        },
        {
          "type": "docker",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#F8FAFC",
          "background": "#06B6D4",
          "template": " \uf308 {{ .Context }} "
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#F8FAFC",
          "background": "#3B82F6",
          "template": " \ufd31 {{ .Context }}{{ if .Namespace }}:{{ .Namespace }}{{ end }} "
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#F8FAFC",
          "background": "#EC4899",
          "trailing_diamond": "\ue0b4",
          "template": " \uf252 {{ .FormattedMs }} ",
          "options": {
            "threshold": 500,
            "style": "roundrock"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "#10B981",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#EF4444{{ end }}"
          ],
          "template": "\u276f ",
          "options": {
            "always_enabled": true
          }
        }
      ]
    }
  ],
  "transient_prompt": {
    "foreground": "#64748B",
    "background": "transparent",
    "template": "\u276f "
  },
  "secondary_prompt": {
    "foreground": "#A78BFA",
    "background": "transparent",
    "template": "\u276f\u276f "
  }
}
